
# API Keys
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Application Configuration
VITE_APP_NAME=Valuation Analyzer
VITE_APP_VERSION=1.0.0

# Development Settings
NODE_ENV=development
VITE_DEV_MODE=true

# Optional: Other Financial Data APIs
VITE_FINANCIAL_API_KEY=your_financial_api_key_here
VITE_MARKET_DATA_API_KEY=your_market_data_api_key_here

# Optional: Database URLs (if using external databases)
DATABASE_URL=your_database_url_here

# Optional: Authentication Settings
VITE_JWT_SECRET=your_jwt_secret_here

# Optional: Email Service (for notifications)
SMTP_HOST=your_smtp_host_here
SMTP_PORT=587
SMTP_USER=your_smtp_user_here
SMTP_PASS=your_smtp_password_here

# Optional: Analytics
VITE_ANALYTICS_ID=your_analytics_id_here

# Note: In Vite projects, only variables prefixed with VITE_ are exposed to the client-side code
# Server-side only variables (like database URLs, secrets) should NOT have the VITE_ prefix
